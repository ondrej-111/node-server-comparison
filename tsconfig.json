{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "./dist",
    "moduleResolution": "node",
    "lib": [
      "esnext.asynciterable"
    ],
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "composite": true,
    "baseUrl": "./",
    "typeRoots": [
      "node_modules/@types",
      "@types"
    ]
  },
  "include": [
    "./app.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "build",
    "config",
    "tmp",
    "logs",
    "plain_server"
  ]
}
